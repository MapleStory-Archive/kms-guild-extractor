<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <StartupObject>KMSGuildExtractor.App</StartupObject>
    <ApplicationIcon>.\resources\icons\icon.ico</ApplicationIcon>
    <Version>0.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <Resource Include="resources\fonts\NanumGothic-Bold.ttf" />
    <Resource Include="resources\fonts\NanumGothic-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="resources\icons\icon.png" />

    <Resource Include="resources\icons\close.png" />
    <Resource Include="resources\icons\minimize.png" />
    <Resource Include="resources\icons\search.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="resources\icons\worlds\arcane.gif" />
    <Resource Include="resources\icons\worlds\aurora.gif" />
    <Resource Include="resources\icons\worlds\bera.gif" />
    <Resource Include="resources\icons\worlds\burning.gif" />
    <Resource Include="resources\icons\worlds\burning2.gif" />
    <Resource Include="resources\icons\worlds\croa.gif" />
    <Resource Include="resources\icons\worlds\elysium.gif" />
    <Resource Include="resources\icons\worlds\enosis.gif" />
    <Resource Include="resources\icons\worlds\luna.gif" />
    <Resource Include="resources\icons\worlds\nova.gif" />
    <Resource Include="resources\icons\worlds\reboot.gif" />
    <Resource Include="resources\icons\worlds\reboot2.gif" />
    <Resource Include="resources\icons\worlds\red.gif" />
    <Resource Include="resources\icons\worlds\scania.gif" />
    <Resource Include="resources\icons\worlds\union.gif" />
    <Resource Include="resources\icons\worlds\zenith.gif" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Localization\LocalizationString.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>LocalizationString.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Localization\LocalizationString.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>LocalizationString.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SciChart.Wpf.UI.Transitionz" Version="3.1.251" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\KMSGuildExtractor.Core\KMSGuildExtractor.Core.csproj" />
  </ItemGroup>

</Project>